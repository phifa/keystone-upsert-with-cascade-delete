// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Sample {
  id   String        @id @default(cuid())
  name String        @unique @default("")
  kids SampleChild[] @relation("Sample_kids")
  cars SampleCar[]   @relation("Sample_cars")
}

model SampleCar {
  id               String   @id @default(cuid())
  name             String   @default("")
  from_Sample_cars Sample[] @relation("Sample_cars")
}

model SampleChild {
  id               String             @id @default(cuid())
  name             String             @default("")
  kiddos           SampleGrandChild[] @relation("SampleChild_kiddos")
  from_Sample_kids Sample[]           @relation("Sample_kids")
}

model SampleGrandChild {
  id                      String                 @id @default(cuid())
  name                    String                 @default("")
  baby                    SampleGreatGrandChild? @relation("SampleGrandChild_baby", fields: [babyId], references: [id])
  babyId                  String?                @map("baby")
  from_SampleChild_kiddos SampleChild[]          @relation("SampleChild_kiddos")

  @@index([babyId])
}

model SampleGreatGrandChild {
  id                         String             @id @default(cuid())
  name                       String             @default("")
  from_SampleGrandChild_baby SampleGrandChild[] @relation("SampleGrandChild_baby")
}

model User {
  id       String @id @default(cuid())
  name     String @default("")
  email    String @unique @default("")
  password String
}